{
    "manifest_version": 3,
    "name": "Pehla",
    "description": "I too started",
    "permissions" : ["activeTab","storage","tabs"],
    "version": "0.7",
    "content_scripts" :
    [
        {
            "matches" :
            [
                "<all_urls>"
            ],
            "js" :[ "content.js" ]
            
        }
    ],
    "background" :
    {
        "service_worker" : "background.js"
    },
    "action": {
        "default_icon": {              
          "128": "logo128.png"   
              
        },
        "default_title": "Click Me",
        "default_popup" : "indeex.html"  
        
      }

    
    
}